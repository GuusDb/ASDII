<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.text.Font?>

<fx:root type="BorderPane" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <center>
      <Label fx:id="labTeller" text="teller" BorderPane.alignment="CENTER">
         <font>
            <Font size="72.0" />
         </font>
      </Label>
   </center>
   <top>
      <Button mnemonicParsing="false" onAction="#resume" prefHeight="33.0" prefWidth="172.0" text="RESUME" BorderPane.alignment="CENTER" />
   </top>
   <bottom>
      <Button mnemonicParsing="false" onAction="#suspend" prefHeight="33.0" prefWidth="172.0" text="SUSPEND" BorderPane.alignment="CENTER" />
   </bottom>
   <right>
      <Button mnemonicParsing="false" onAction="#stop" prefHeight="109.0" prefWidth="52.0" text="STOP" BorderPane.alignment="CENTER" />
   </right>
</fx:root>
